---
import Layout from '../layouts/Layout.astro';

interface Props {
    title: string;
    description: string;
    url: string;
}

const { title, description, url } = Astro.props;
---

<Layout
    title={title}
    description={description}
    disableDefaultBackground
>
    <iframe 
        class="h-dvh w-full bg-black"
        src={url}
        title={title}
    ></iframe>
</Layout> 